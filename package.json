{
  "name": "single-stone-skills-api",
  "version": "1.0.0",
  "description": "employee skills api",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "nodemon --exec npm run restart",
    "restart": "rimraf dist && npm run build && npm run start",
    "start": "node dist/index.js",
    "lint": "eslint ./src --ext .js",
    "test": "test",
    "make-model": "npx sequelize-cli model:generate --name Name --attributes first_name:string,last_name:string",
    "migrate-db": "npx sequelize-cli db:migrate",
    "undo-dbMigration": "npx sequelize-cli db:migrate:undo:all",
    "make-seed":"npx sequelize-cli seed:generate --name demo-user",
    "seed-db": "npx sequelize-cli db:seed:all",
    "undo-dbSeed": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GRamirez2/ssApi.git"
  },
  "keywords": [
    "api",
    "node"
  ],
  "author": "George Ramirez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GRamirez2/ssApi/issues"
  },
  "homepage": "https://github.com/GRamirez2/ssApi#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.16.2"
  }
}
